{
  "name": "react-inspector/metric",
  "description": "\ud83d\udcd0 Metric VO's",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "https://www.wyrihaximus.net/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "thecodingmachine/safe": "^1.0"
  },
  "require-dev": {
    "icanhazstring/composer-unused": "^0.7",
    "infection/infection": "^0.16.0",
    "maglnet/composer-require-checker": "^2.0",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "phpstan/phpstan": "^0.12.99",
    "squizlabs/php_codesniffer": "^3.6",
    "vimeo/psalm": "^3.4",
    "wyrihaximus/test-utilities": "^2.0"
  },
  "config": {
    "platform": {
      "php": "7.4"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "php"
    ]
  },
  "autoload": {
    "psr-4": {
      "ReactInspector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReactInspector\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
